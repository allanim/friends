  "plugins": [
    "toggle-chapters",
    "insert-logo-link",
    "favicon",
    "back-to-top-button",
    "ga"
  ],
  "pluginsConfig": {
    "insert-logo-link": {
      "src": "/img/friends_logo.png",
      "url": "/"
    },
    "favicon":{
        "shortcut": "/img/favicon.ico",
        "bookmark": "/img/favicon.ico",
